upstream backend {
    
    # Adjust the following list to the list of the service instances.
    
    server 127.0.0.1:8081 weight=1;
    server 127.0.0.1:8082 weight=1;
    server 127.0.0.1:8083 weight=10;
}

server {
    listen 80;
    server_name _;

    location / {
        proxy_pass http://backend;
    }
}
